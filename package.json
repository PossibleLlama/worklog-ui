{
  "name": "worklog-ui",
  "version": "0.0.4",
  "description": "UI for discovering trends on what you have been doing. Part of the worklog project.",
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build",
    "test": "jest --detectOpenHandles",
    "test:all": "npm run test:type && npm run test:lint && npm test",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:type": "tsc",
    "test:lint": "eslint src/**/*.ts{,x}",
    "lint-fix": "eslint src/**/*.ts{,x} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PossibleLlama/worklog-ui.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Chris Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PossibleLlama/worklog-ui/issues"
  },
  "homepage": "https://github.com/PossibleLlama/worklog-ui",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.37.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.5.2"
  }
}
