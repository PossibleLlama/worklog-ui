{
  "name": "worklog-ui",
  "version": "0.0.1",
  "description": "UI for discovering trends on what you have been doing. Part of the worklog project.",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --watch",
    "test:coverage": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --coverage",
    "test:type": "tsc",
    "test:lint": "eslint src/**/*.ts{,x} -f table",
    "lint-fix": "eslint src/**/*.ts{,x} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PossibleLlama/worklog-ui.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Chris Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PossibleLlama/worklog-ui/issues"
  },
  "homepage": "https://github.com/PossibleLlama/worklog-ui#readme",
  "dependencies": {
    "@zendeskgarden/react-datepickers": "^8.52.0",
    "@zendeskgarden/react-forms": "^8.52.0",
    "@zendeskgarden/react-modals": "^8.52.0",
    "axios": "^0.21.4",
    "date-fns": "^2.28.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.3"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^12.1.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@web/test-runner": "^0.13.17",
    "@web/test-runner-playwright": "^0.8.8",
    "@zendeskgarden/react-buttons": "^8.40.0",
    "@zendeskgarden/react-grid": "^8.40.0",
    "@zendeskgarden/react-notifications": "^8.40.0",
    "@zendeskgarden/react-tags": "^8.40.0",
    "@zendeskgarden/react-theming": "^8.40.0",
    "@zendeskgarden/react-typography": "^8.40.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.2",
    "react-component-octicons": "^1.8.0",
    "snowpack": "https://gitpkg.now.sh/gpascale/snowpack/snowpack?cjs-require-fix-pkg",
    "styled-components": "^5.3.1",
    "typescript": "^4.4.3"
  }
}
