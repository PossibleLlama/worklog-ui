{
  "name": "worklog-ui",
  "version": "0.0.2",
  "description": "UI for discovering trends on what you have been doing. Part of the worklog project.",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --watch",
    "test:coverage": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --coverage",
    "test:type": "tsc",
    "test:lint": "eslint src/**/*.ts{,x}",
    "lint-fix": "eslint src/**/*.ts{,x} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PossibleLlama/worklog-ui.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Chris Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PossibleLlama/worklog-ui/issues"
  },
  "homepage": "https://github.com/PossibleLlama/worklog-ui#readme",
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@zendeskgarden/react-datepickers": "^8.53.1",
    "@zendeskgarden/react-forms": "^8.53.0",
    "@zendeskgarden/react-modals": "^8.53.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "@web/test-runner": "^0.13.31",
    "@web/test-runner-playwright": "^0.8.9",
    "@zendeskgarden/react-buttons": "^8.53.0",
    "@zendeskgarden/react-grid": "^8.53.0",
    "@zendeskgarden/react-notifications": "^8.53.0",
    "@zendeskgarden/react-tags": "^8.53.0",
    "@zendeskgarden/react-theming": "^8.53.0",
    "@zendeskgarden/react-typography": "^8.53.0",
    "autoprefixer": "^10.4.7",
    "chai": "^4.3.6",
    "eslint": "^8.20.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}
