{
  "name": "worklog-ui",
  "version": "0.0.1",
  "description": "UI for discovering trends on what you have been doing. Part of the worklog project.",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --watch",
    "test:coverage": "web-test-runner \"src/**/*.test.ts{,x}\" --node-resolve --playwright --browsers chromium firefox webkit --coverage",
    "test:type": "tsc",
    "test:lint": "eslint src/**/*.ts{,x} -f table",
    "lint-fix": "eslint src/**/*.ts{,x} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PossibleLlama/worklog-ui.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Chris Collier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PossibleLlama/worklog-ui/issues"
  },
  "homepage": "https://github.com/PossibleLlama/worklog-ui#readme",
  "dependencies": {
    "@zendeskgarden/react-datepickers": "^8.53.1",
    "@zendeskgarden/react-forms": "^8.53.0",
    "@zendeskgarden/react-modals": "^8.53.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.3"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^12.1.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.36",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@web/test-runner": "^0.13.28",
    "@web/test-runner-playwright": "^0.8.8",
    "@zendeskgarden/react-buttons": "^8.52.0",
    "@zendeskgarden/react-grid": "^8.52.0",
    "@zendeskgarden/react-notifications": "^8.52.0",
    "@zendeskgarden/react-tags": "^8.52.0",
    "@zendeskgarden/react-theming": "^8.52.0",
    "@zendeskgarden/react-typography": "^8.52.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "react-component-octicons": "^1.8.0",
    "snowpack": "https://gitpkg.now.sh/gpascale/snowpack/snowpack?cjs-require-fix-pkg",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.2"
  }
}
